          },
          {
            "action": "COMPUTE_TOTAL_FROM_COUNT",
            "inputs": { "amount_field": "per_ticket_amount_usd", "count_field": "eligible_ndc_ticket_count" },
            "output": { "field": "payout_amount", "unit": "USD", "explanation": "payout_amount = per_ticket_amount * eligible_ndc_ticket_count" }
          }
        ]

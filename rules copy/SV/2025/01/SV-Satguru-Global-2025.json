  "rules": [
    {
      "rule_id": "2541b1fa-b408-4066-a951-f13813d00edc_MTP1",
      "name": "SV Target-Based Incentive – Net Flown Revenue (AED)",
      "type": "Multi Tier Metric TABLE",
      "variant_flags": {
        "Sales": false,
        "Coupon Count": false,
        "Ticket Count": false,
        "NDC Share": false,
        "Ancillary": false,
        "Corporate Code": false,
        "NFR": true,
        "Combination": false,
        "Combination_detail": "NONE"
      },
      "what_if": {
        "active_window": { "from": "2025-01-01", "to": "2025-12-31" },
        "trigger": {
          "type": "FLOWN",
          "measure": ["Net Flown Revenue"],
          "components": ["BASE","YQ","YR"],
          "formula": "NRF = BASE + YQ + YR (excludes upfront commission; exchanged/reissued tickets included)",
          "capping": { "enabled": true, "value": "last_tier_target_plus_10pct_of_trigger" }
        },
        "payout": {
          "type": "PERCENTAGE",
          "computation": "INCREMENTAL",
          "measure": ["Net Flown Revenue"],
          "components": ["BASE","YQ","YR"],
          "formula": "payout_amount = tier_percent * (BASE + YQ + YR)",
          "expected_percent_range": { "min": 1.0, "max": 7.0, "logic": "as per NFR slabs" },
          "capping": { "enabled": true, "value": "last_tier_target_plus_10pct_of_trigger" }
        }
      },
      "where_trigger": {
        "IN": {
          "Fare Type": ["IT","BT","NEGO","GROUPS(G)","CORPORATE"],
          "Sales Date": "2025-01-01 to 2025-12-31",
          "Travel Date": "2025-01-01 to 2025-12-31",
          "Marketing Airline": ["SV"],
          "Ticketing Airline": ["SV"],
          "Date Notes": ["Carrier Routing = Other"]
        },
        "OUT": {
          "Fare Type": ["GOV","HAJJ","FULL CHARTER"],
          "Tour Code": ["GOV","SVMRNE"],
          "Fare Basis Patterns": ["*GR*"],
          "Other": ["Bookings not issued by Agency CRS/IATA location"]
        },
        "SILENT": {
          "Cabin": ["ALL"],
          "RBD": ["ALL"],
          "POS": ["ALL"],
          "POO": ["ALL"],
          "Route": ["ALL"],
          "O&D": ["ALL"],
          "Flight Nos": ["ALL"],
          "Alliance": ["NONE"],
          "Code Share": ["ALLOW"],
          "Interline": ["ALLOW"],
          "NDC": ["ANY"]
        }
      },
      "where_payout": {
        "IN": {
          "Sales Date": "2025-01-01 to 2025-12-31",
          "Travel Date": "2025-01-01 to 2025-12-31",
          "Marketing Airline": ["SV"],
          "Ticketing Airline": ["SV"]
        },
        "OUT": {
          "Fare Type": ["GOV","HAJJ","FULL CHARTER"],
          "Tour Code": ["GOV","SVMRNE"],
          "Fare Basis Patterns": ["*GR*"],
          "Other": ["EMD/VMPD", "Tickets in other incentive deals", "Pure domestic tickets unless otherwise specified"]
        }
      },
      "tiers": [
        {
          "table_label": "Target-Based Incentives – NFR (AED)",
          "table_filter": { "key": "NONE", "value": [] },
          "rows": [
            { "metric": "Net Flown Revenue", "target": { "min": 0, "max": 51717076.99 }, "payout": { "value": 1.0, "unit": "PERCENT" } },
            { "metric": "Net Flown Revenue", "target": { "min": 51717077.0, "max": 56371613.99 }, "payout": { "value": 3.0, "unit": "PERCENT" } },
            { "metric": "Net Flown Revenue", "target": { "min": 56371614.0, "max": 60508979.99 }, "payout": { "value": 3.5, "unit": "PERCENT" } },
            { "metric": "Net Flown Revenue", "target": { "min": 60508980.0, "max": 63612004.99 }, "payout": { "value": 4.5, "unit": "PERCENT" } },
            { "metric": "Net Flown Revenue", "target": { "min": 63612005.0, "max": 66999999.99 }, "payout": { "value": 6.0, "unit": "PERCENT" } },
            { "metric": "Net Flown Revenue", "target": { "min": 67000000.0, "max": "INF" }, "payout": { "value": 7.0, "unit": "PERCENT" } }
          ]
        }
      ],
      "then": {
        "evaluation": { "basis": "INCREMENTAL", "measure_name": "Net Flown Revenue", "apply_components": ["BASE","YQ","YR"] },
        "actions": [
          {
            "action": "COMPUTE_PAYOUT_PERCENT",
            "inputs": {
              "slab_table": "Target-Based Incentives – NFR (AED)",
              "metric": "Net Flown Revenue",
              "respect_capping": true,
              "cap_rule": "last_tier_target_plus_10pct_of_trigger"
            },
            "output": {
              "field": "payout_amount",
              "unit": "AED",
              "explanation": "payout_amount = tier_percent * (BASE + YQ + YR)"
            }
          }
        ]
      },
      "guards": {
        "contract_cap": { "enabled": true, "value": "last_tier_target_plus_10pct_of_trigger", "notes": "Payment capped at last tier +10% of trigger; disputes within 90 days of ACM; tax compliance required." }
      }
    },

    {

    "rules": [
      {
        "rule_id": "7d4b767d-d661-4c2d-a8a0-b422dc198fbd_MTP1",
        "name": "Q1 Growth – Differential on Flown (BASE+YR); YoY tiers vs BLP",
        "type": "Multi Tier Metric TABLE",
        "variant_flags": {
          "Sales": false,
          "Coupon Count": false,
          "Ticket Count": false,
          "NDC Share": false,
          "Ancillary": false,
          "Corporate Code": false,
          "NFR": false,
          "Combination": false,
          "Combination_detail": "NONE"
        },
  
        "blp": {
          "applies": true,
          "value": 38074,
          "notes": "BLP is PY Q1 2024 Net Flown Revenue (USD). YoY Growth % is calculated as (Q1 2025 flown_revenue / 38074) * 100."
        },
  
        "what_if": {
          "active_window": { "from": "2025-01-01", "to": "2025-03-31" },
          "trigger": {
            "type": "FLOWN",
            "measure": ["Net Flown Revenue"],
            "components": ["BASE","YR"],
            "formula": "flown_revenue = BASE + YR; yoy_growth_percent = (flown_revenue / BLP) * 100",
            "capping": { "enabled": true, "value": "120% YoY cap" }
          },
          "payout": {
            "type": "PERCENTAGE",
            "computation": "DIFFERENTIAL",
            "measure": ["Net Flown Revenue"],
            "components": ["BASE","YR"],
            "formula": "payout_amount = tier_percent * revenue_in_selected_yoy_band",
            "expected_percent_range": { "min": 3.0, "max": 3.5, "logic": "Per Q1 YoY bands" },
            "capping": { "enabled": true, "value": "No payout beyond 120% YoY" }
          }
        },
  
        "where_trigger": {
          "IN": {
            "Fare Type": ["Published","Promotional(PR)","Corporate"],
            "Sales Date": "2025-01-01 to 2025-03-31",
            "Travel Date": "2025-01-01 to 2025-03-31",
            "City Codes": ["MPM"],
            "POS": ["MZ"],
            "Marketing Airline": ["KQ"],
            "Operating Airline": ["KQ"],
            "Ticketing Airline": ["KQ-706"]
          },
          "OUT": {
            "Fare Type": ["Upfront Discounted","IT","Groups","Missionary","Student","Seamen","Infant","Free tickets"],
            "Code Share": ["Excluded if not flown on KQ metal"],
            "Interline": ["Excluded"]
          },
          "SILENT": {
            "Cabin": ["ANY"],
            "RBD": ["ANY"],
            "Dom/Intl": ["ANY"],
            "O&D": ["ANY"],
            "Route": ["ANY"],
            "Flight Nos": ["ANY"],
            "Corporate Code": ["ANY"],
            "Tour Code": ["ANY"],
            "PCCs": ["ANY"],
            "POO": ["ANY"],
            "Alliance": ["ANY"]
          }
        },
  
        "where_payout": {
          "IN": {
            "Ticketing Airline": ["KQ-706"],
            "POS": ["MZ"]
          },
          "OUT": {},
          "SILENT": {}
        },
  
        "tiers": [
          {
            "table_label": "Q1 YoY Growth Bands vs BLP (BLP = 38,074 USD)",
            "table_filter": { "key": "Quarter", "value": ["Q1 2025"] },
            "rows": [
              { "metric": "YoY Growth %", "target": { "min": 100, "max": 110 }, "payout": { "value": 3.0, "unit": "PERCENT" } },
              { "metric": "YoY Growth %", "target": { "min": 111, "max": 120 }, "payout": { "value": 3.5, "unit": "PERCENT" } }
            ]
          }
        ],
  
        "then": {
          "evaluation": { "basis": "DIFFERENTIAL", "measure_name": "Net Flown Revenue", "apply_components": ["BASE","YR"] },
          "actions": [
            {
              "action": "COMPUTE_PAYOUT_PERCENT",
              "inputs": {
                "slab_table": "Q1 YoY Growth Bands vs BLP (BLP = 38,074 USD)",
                "metric": "YoY Growth %",
                "respect_capping": true,
                "cap_value": "120"
              },
              "output": {
                "field": "payout_amount",
                "unit": "USD",
                "explanation": "payout_amount = tier_percent * revenue_in_selected_yoy_band"
              }
            }
          ]
        },
  
        "guards": {
          "contract_cap": {
            "enabled": true,
            "value": "120% YoY",
            "notes": "No accrual or payment above maximum YoY threshold."
          },
          "performance_guards": [
            "Carrier statistics are determinative.",
            "Infant and free tickets excluded.",
            "YR included in measure; YQ, taxes, commissions excluded."
          ]
        }
      },
  

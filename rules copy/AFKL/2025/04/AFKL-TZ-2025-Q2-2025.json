  "rules": [
    {
      "rule_id": "AFKL-TZ-2025-Q1_RULESET_MTP1",
      "name": "Q1 Growth - Economy Light (Incremental on Sold, BASE+YR)",
      "type": "Multi Tier Metric TABLE",

      "variant_flags": {
        "Sales": true,
        "Coupon Count": false,
        "Ticket Count": false,
        "NDC Share": false,
        "Ancillary": false,
        "Corporate Code": false,
        "NFR": false,
        "Combination": false,
        "Combination_detail": "NONE"
      },

      "blp": {
        "applies": true,
        "value": "PY Sold Revenue for same quarter in 2024",
        "notes": "Baseline defined as previous-year same-quarter Sold Revenue."
      },

      "what_if": {
        "active_window": { "from": "2025-01-01", "to": "2025-03-31" },

        "trigger": {
          "type": "SALES",
          "measure": ["YoY Growth %"],
          "components": ["BASE", "YR"],
          "formula_key": "F003",
          "capping": { "enabled": true, "value": "4170" }
        },

        "payout": {
          "type": "PERCENTAGE",
          "computation": "INCREMENTAL",
          "measure": ["Sold Revenue"],
          "components": ["BASE", "YR"],
          "formula_key": "F007",
          "expected_percent_range": {
            "min": 0.7,
            "max": 1.75,
            "logic": "Percent values are percentage points (e.g. 1.25 = 1.25%)."
          },
          "capping": { "enabled": true, "value": "4170" }
        }
      },

      "where_trigger": {
        "IN": {
          "Cabin": ["Economy Light"],
          "POS": ["TZ"],
          "POO": ["TZ"],
          "Marketing_Airline": ["AF", "KL"],
          "Operating_Airline": ["AF", "KL", "DL", "KQ", "HV"],
          "Ticketing_Airline": ["AF-057", "KL-074"],
          "Sales_Date": { "start": "2025-01-01", "end": "2025-03-31" },
          "Travel_Date": { "start": "2025-01-01", "end": "2025-03-31" }
        },
        "OUT": {
          "Other": [
            "YQ distribution surcharge OUT",
            "Taxes and fees OUT",
            "SAF contribution OUT",
            "Commissions OUT",
            "ADMs/ACMs OUT",
            "Ancillaries OUT",
            "Infant/free tickets OUT"
          ]
        },
        "SILENT": {}
      },

      "where_payout": {
        "IN": {
          "Cabin": ["Economy Light"],
          "POS": ["TZ"],
          "POO": ["TZ"]
        },
        "OUT": {
          "Other": [
            "YQ distribution surcharge OUT",
            "Taxes and fees OUT",
            "SAF contribution OUT"
          ]
        },
        "SILENT": {}
      },

      "tiers": [
        {
          "table_label": "Q1 YoY Growth - Economy Light",
          "table_filter": { "key": "Traffic Area", "value": ["From POO TZ to ALL"] },
          "rows": [
            { "metric": "YoY Growth %", "target": { "min": 101, "max": 110 }, "payout": { "value": 0.7, "unit": "PERCENT" } },
            { "metric": "YoY Growth %", "target": { "min": 110, "max": 120 }, "payout": { "value": 1.23, "unit": "PERCENT" } },
            { "metric": "YoY Growth %", "target": { "min": 120, "max": 130 }, "payout": { "value": 1.75, "unit": "PERCENT" } }
          ]
        }
      ],

      "then": {
        "evaluation": {
          "basis": "INCREMENTAL",
          "measure_name": "Sold Revenue",
          "apply_components": ["BASE", "YR"],
          "formula_key": "F007"
        },
        "actions": [
          {
            "action": "COMPUTE_PAYOUT_PERCENT",
            "inputs": {
              "slab_table": "Q1 YoY Growth - Economy Light",
              "metric": "YoY Growth %",
              "respect_capping": true,
              "cap_value": "4170"
            },
            "output": {
              "field": "payout_amount",
              "unit": "USD",
              "explanation": "Apply payout formula key F007 with INCREMENTAL computation on eligible Sold Revenue (BASE + YR), respecting cap."
            }
          }
        ]
      },

      "guards": {
        "contract_cap": {
          "enabled": true,
          "value": "4170",
          "notes": "Maximum incentive amount per cabin for the Reference Period."
        },
        "performance_guards": [
          "Only Q1 Reference Period applies",
          "Traffic restricted to POO TZ"
        ]
      }
    },

